{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "composite": true,
        "declaration": true,
        // "emitDeclarationOnly": true,
        // "isolatedModules": true,
        "sourceMap": true,
        "outDir": "./dist",

        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "downlevelIteration": true,
        "strictNullChecks": true,
        "strictFunctionTypes": false,
        "skipLibCheck": true
    }
}