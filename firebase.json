{
  "functions": {
    "predeploy": [
      "yarn lint",
      "yarn --cwd functions build"
    ],
    "source": "functions/dist"
  },
  "hosting": {
    "public": "web-react/dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}
