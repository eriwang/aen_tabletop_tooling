    "deploy": "yarn firebase deploy --only functions",
    "logs": "yarn firebase functions:log"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "7.6.0",
    "eslint-plugin-import": "^2.25.3",
    "firebase-tools": "^9.23.1",
    "jest": "^27.4.4",
    "jest-when": "^3.4.2",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "ts-jest": "^27.1.1",
    "yarn": "^1.22.17"
  }
}
